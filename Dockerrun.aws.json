{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "m4r14r1os/complex-multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "m4r14r1os/complex-multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links": ["client","server"]
        },
        {
            "name": "server",
            "image": "m4r14r1os/complex-multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "m4r14r1os/complex-multi-worker",
            "hostname": "worker",
            "essential": false
        }
    ]
}